<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kino"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Class="Kino.DodajFilm"
        mc:Ignorable="d"
        Title="DodajFilm" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textbox_nazwa"  HorizontalAlignment="Left" Height="40" Margin="66,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="textbox_gatunek" HorizontalAlignment="Left" Height="40" Margin="66,102,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="textbox_rok" HorizontalAlignment="Left" Height="40" Margin="66,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="textbox_opis" HorizontalAlignment="Left" Height="132" Margin="66,190,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300"/>



        <xctk:DateTimePicker x:Name="date_film" HorizontalAlignment="Left" Margin="66,327,0,0" VerticalAlignment="Top" Height="31" Width="227"/>
        <Button x:Name="button_dodajfilm" Content="Dodaj film!" HorizontalAlignment="Left" Margin="402,339,0,0" VerticalAlignment="Top" Width="75" Click="Button_dodajfilm_Click"/>



    </Grid>
</Window>
